{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [

    {
      "library": "node_modules/devextreme/dist/css/icons/",
      "destination": "wwwroot/css/icons",
      "files": [
        "dxicons.ttf",
        "dxicons.woff2",
        "dxicons.woff"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.15.3",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "cdnjs",
      "library": "datepicker@1.0.10",
      "destination": "wwwroot/lib/datepicker/"
    }
  ]
}